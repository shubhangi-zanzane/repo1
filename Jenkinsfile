pipeline {
    agent any
    triggers {
        githubPush()
    }
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/shubhangi-zanzane/project1.git'
            }
        }
        stage('Build') {
            steps {
              sh 'echo "Building..."'
            }
        }
    }
}
